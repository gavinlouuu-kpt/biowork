# Label Studio Development Environment Variables
# Copy this file to .env.development and fill in your values

# ML Backend Configuration
# Set to true to enable automatic ML backend connection for new projects
ADD_DEFAULT_ML_BACKENDS=true

# URL of your default ML backend server
DEFAULT_ML_BACKEND_URL=http://localhost:8080

# Display name for the default ML backend
DEFAULT_ML_BACKEND_TITLE=Default ML Backend

# Optional: Database configuration (uncomment and modify if needed)
# DATABASE_NAME=label_studio.db
# DATABASE_TYPE=sqlite

# Optional: Debug settings (uncomment and modify if needed)
# DEBUG=true
# LOG_LEVEL=DEBUG
